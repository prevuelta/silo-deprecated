{
    "name": "Stardome-CMS",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "watch-cms": "run-p watch-cms:sass watch-cms:serve watch-cms:js sync-cms watch-cms:lib",
        "serve-cms": "node ./cms/app/app.js",
        "watch-cms:serve": "nodemon --ignore 'test/*' ./cms/app/app.js",
        "build-cms": "run-s build-cms:lib build-cms:js build-cms:css",
        "watch-cms:sass": "sass --watch ./cms/client/styles/main.scss:./cms/client/public/main.css",
        "build-cms:css": "sass ./cms/client/styles/main.scss:./cms/client/public/main.css",
        "watch-cms:js": "nodemon --watch ./cms/client/scripts -x 'npm run build-cms:js' ",
        "watch-cms:lib": "nodemon --watch ./cms/rollup.config.lib.js -x 'npm run build-cms:lib'",
        "build-cms:js": "echo 'Building js';rollup -c ./cms/rollup.config.admin.js",
        "build-cms:lib": "echo 'Building lib'; rollup -c ./cms/rollup.config.lib.js",
        "sync-cms": "browser-sync start --port 4101 --proxy 'localhost:8030' --files ./cms/client"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {},
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ]
    },
    "dependencies": {
        "@babel/core": "^7.1.6",
        "@babel/preset-env": "^7.1.6",
        "@babel/preset-react": "^7.0.0",
        "ajv": "^6.1.1",
        "ajv-merge-patch": "^4.1.0",
        "babel-plugin-object-rest-spread": "0.0.0",
        "babel-plugin-transform-object-assign": "^6.22.0",
        "bcrypt": "^3.0.2",
        "bcrypt-nodejs": "0.0.3",
        "bluebird": "^3.5.3",
        "body-parser": "^1.18.1",
        "browser-sync": "^2.26.3",
        "cookie-parser": "^1.4.3",
        "cookie-session": "^2.0.0-beta.3",
        "dotenv": "^6.2.0",
        "express": "^4.14.0",
        "express-session": "^1.14.1",
        "file-type": "^10.6.0",
        "filesize": "^3.6.1",
        "flash": "^1.1.0",
        "glob": "^7.0.6",
        "gm": "^1.23.0",
        "http-status-codes": "^1.3.0",
        "isomorphic-fetch": "^2.2.1",
        "joi": "^14.0.6",
        "js2xmlparser": "^4.0.0",
        "json-schema-ref-parser": "^6.0.2",
        "jsonwebtoken": "^8.0.1",
